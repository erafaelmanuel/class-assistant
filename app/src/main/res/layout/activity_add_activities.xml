<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <include
            layout="@layout/main_header_toolbar"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">



                <TextView
                    android:layout_gravity="left|center"
                    android:layout_marginLeft="25dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Cancel"
                    android:textSize="18dp"
                    android:textStyle="bold"
                    android:textColor="@color/colorWhite"/>
            </FrameLayout>


        </LinearLayout>

    </android.support.v7.widget.CardView>


    <android.support.v7.widget.CardView
        android:layout_marginBottom="2dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout

            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/icon_calendar" />

                <TextView
                    android:id="@+id/input_date5"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="15dp"
                    android:text="--/--/--"
                    android:textSize="18sp"
                    android:textStyle="bold" />


            </LinearLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <Button
                    android:layout_marginTop="5dp"
                    android:layout_marginRight="16dp"
                    android:layout_gravity="right"
                    android:text="Change"
                    android:layout_width="100dp"
                    android:layout_height="40dp" />

            </FrameLayout>
        </LinearLayout>

    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <FrameLayout
                android:id="@+id/input_detailts5"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_gravity="left"
                        android:layout_marginLeft="10dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <TextView
                            android:layout_marginTop="10dp"
                            android:textSize="16dp"
                            android:layout_gravity="center"
                            android:text="Title :"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />

                        <TextView
                            android:layout_marginTop="20dp"
                            android:layout_gravity="center"
                            android:text="Total :"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="16sp"/>

                    </LinearLayout>

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_marginLeft="10dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <EditText
                            android:id="@+id/input_name5"
                            android:hint="Project"
                            android:textSize="16dp"
                            android:textColorHint="#000000"
                            android:layout_width="180dp"
                            android:layout_height="wrap_content" />

                        <EditText
                            android:id="@+id/input_total5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:hint="Total"
                            android:textColorHint="#000000"
                            android:inputType="number"
                            android:textSize="16dp"
                            />
                    </LinearLayout>

                </LinearLayout>

            </FrameLayout>

            <Button
                android:id="@+id/btn_ok_start"
                android:clickable="false"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="10dp"
                android:background="#2980b9"
                android:text="Ok"
                android:layout_gravity="center"
                android:layout_width="100dp"
                android:layout_height="35dp" />
        </LinearLayout>
    </android.support.v7.widget.CardView>

    <FrameLayout
        android:background="@color/colorClouds"
        android:layout_width="match_parent"
        android:layout_height="10dp">

    </FrameLayout>


    <android.support.v7.widget.CardView
        android:layout_marginTop="10dp"
        android:id="@+id/input_failedemp5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:background="#3498db"
            android:layout_gravity="center"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="10dp"
                    android:layout_gravity="center"
                    android:orientation="vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="5dp"
                        android:layout_gravity="center"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Start with call by Name"
                        android:textSize="14dp"
                        android:textColor="#000000"/>
                </LinearLayout>
            </FrameLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <Button
                    android:id="@+id/btn_startbyName"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="10dp"
                    android:text="Start"
                    android:textColor="#FFFFFF"
                    android:background="#c0392b"
                    android:layout_gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"

                    />
            </FrameLayout>
        </LinearLayout>

    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:layout_marginTop="10dp"
        android:id="@+id/input_failed5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:background="#3498db"
            android:layout_gravity="center"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="10dp"
                    android:layout_gravity="center"
                    android:orientation="vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="5dp"
                        android:layout_gravity="center"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Start with by Alpabetical"
                        android:textSize="14dp"
                        android:textColor="#000000"/>

                </LinearLayout>

            </FrameLayout>


            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <Button
                    android:id="@+id/btn_startbyAlpha"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="10dp"
                    android:text="Start"
                    android:textColor="#FFFFFF"
                    android:background="#c0392b"
                    android:layout_gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"

                    />
            </FrameLayout>
        </LinearLayout>

    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:layout_marginTop="10dp"
        android:id="@+id/input_succes5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:background="#3498db"
            android:layout_gravity="center"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <FrameLayout
                android:layout_marginBottom="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView

                    android:gravity="center"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="5dp"
                    android:layout_gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Start with by seating arranemwnt"
                    android:textSize="14dp"
                    android:textColor="#000000"/>


            </FrameLayout>


            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <Button
                    android:background="#bdc3c7"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_gravity="right"
                    android:text="Setup"
                    android:layout_width="80dp"
                    android:layout_height="30dp" />

                <Button
                    android:id="@+id/btn_startbySeat"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="10dp"
                    android:text="Start"
                    android:textColor="#FFFFFF"
                    android:background="#c0392b"
                    android:layout_gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    />
            </FrameLayout>
        </LinearLayout>

    </android.support.v7.widget.CardView>
</LinearLayout>